                           )
                         ; eval-local-path-short(
                           | UnknownParent()
                           , all-parents5
                           , attr-cache5
                           , <get-rewrite-mode(|attr-cache5)>
                           ))
                        ; (?""
                           <+ <conc-strings> (<id>, "."))>
                     , <to-tiny-string>
                     , " (copy rule)"
                     )
    )
    ; (!parent0 ; not(UnknownParent)
       <+ get-parent(|all-parents0))
    ; not(NoParent)
    ; eval-inh-ancestor(
        p
      | <get-parent(|all-parents0) <+ !UnknownParent()>
      , all-parents0
