  eval-inh-ancestor-00(p|parent0, all-parents0, attr-cache0, rewrite-mode0) =
    ?node0
    ; where(id)
    ; ?tree-access-ancestors
    ; (((!<id>
         ; (?node0 ; !parent0 ; not(UnknownParent)
            <+ get-parent(|all-parents0))
         ; not(NoParent))
        ; p
        <+ (?node0
            < (?node0 ; !parent0 ; not(UnknownParent)
