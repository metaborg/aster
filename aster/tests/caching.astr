module caching

strategies

  main-caching =
    option-wrap(
      verbose-option
    ,
      root := "root".init;
      <debug(!1); eq>      (root.get-new-1, root.get-new-1);
      <debug(!2); eq>      (root.get-new-2, root.get-new-2);
      <debug(!3); not(eq)> (root.get-new-3, root.get-new-3)
    )

attributes
  
  local get-new-1 = new
  
  `noop` get-new-2 = new
  
  `uncached` get-new-3 = new
  
attributes

  local `noop`(s) = s
