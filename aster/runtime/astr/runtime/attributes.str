module astr/runtime/attributes

imports
  libstratego-lib
  astr/runtime/hashtables

signature constructors
  EVAL_FAILED : Term

strategies

  get-cached-attribute(|attr-cache, node, name-args) =
    hashtable-get(|attr-cache, (node, name-args))
  
  set-cached-attribute(|attr-cache, node, name-args, value) =
    hashtable-put(|attr-cache, (node, name-args), value)

  attr-cache =
    lookup-table(|"attr-cache")
