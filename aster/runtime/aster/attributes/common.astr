module aster/attributes/assert

imports
  libstratego-lib
  aster/decorators/common
  aster/decorators/flow

attributes   
   
  def plain all(a) =
    ?before;
    all(a);
    (?before <+ id.init)
  
  def plain some(a) =
    ?before;
    some(a);
    (?before <+ id.init)
