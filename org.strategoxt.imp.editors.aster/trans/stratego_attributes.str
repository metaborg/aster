module stratego_attributes

imports
  libstratego-lib
  libstratego-gpp
  libstratego-aterm
  Stratego-Attributes
  editor-common.generated
  
rules
  
  editor-analyze:
    (ast, path, project-path) -> (ast, [], [], [])
  
  generate-aterm:
    (selected, position, ast, path, project-path) -> (filename, result)
    with
      filename := <guarantee-extension(|"aterm")> path;
      result   := selected
